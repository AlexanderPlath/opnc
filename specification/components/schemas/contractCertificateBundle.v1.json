    "properties": {
        "pcid": {
            "$ref": "../parameters/pcidParam.v1.json"
        },
        "emaid": {
            "$ref": "../parameters/emaidParam.v1.json"
        },
        "contractCertificateChain": {
            "$ref": "./certificateChain.v1.json"
        },
        "contractSignatureEncryptedPrivateKey": {
